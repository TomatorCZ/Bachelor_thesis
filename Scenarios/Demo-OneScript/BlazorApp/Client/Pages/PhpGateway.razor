@page "/php/{*sth}"
@using PhpBlazor

<PhpScriptProvider ContextLifetime="@SessionLifetime.OnNavigationChanged" Type="@PhpScriptProviderType.ScriptProvider" >
    <Navigating>
        <p>Navigating</p>
    </Navigating>
    <NotFound>
        <p>Script not found</p>
    </NotFound>
</PhpScriptProvider>

@code
{     
    [Parameter]
    public string sth { get; set; }
}